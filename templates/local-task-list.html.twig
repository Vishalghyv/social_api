{% if items is not empty %}
    <ul class="admin-list">
        {% for item in items %}
            {% set url = path(item.route_name) %}
            <li class="clearfix"><a href="{{ url }}">
                <span class="label">{{ item.title }}</span>
            </a></li>
        {% endfor %}
    </ul>
{% else %}
    <p>
        {% trans %}
        You do not have any enabled module related to this functionality
        {% endtrans %}
    </p>
{% endif %}
